{
  "name": "visual-compiler",
  "version": "1.0.0",
  "description": "AI-powered layout analysis tool that provides precise geometric feedback for UI components",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "visual-compiler": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "init": "npm run build && node dist/index.js init",
    "demo": "npm run build && node dist/index.js demo",
    "watch": "npm run build && node dist/index.js watch",
    "analyze": "npm run build && node dist/index.js analyze",
    "test": "npm run build && npm run demo && npm run analyze test-components/good-layout.html"
  },
  "keywords": [
    "visual-compiler",
    "layout-analysis",
    "ui-testing",
    "responsive-design",
    "ai-tools",
    "component-testing",
    "spatial-analysis",
    "playwright",
    "headless-browser"
  ],
  "author": "Visual Compiler Team",
  "license": "MIT",
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-modules-commonjs": "^7.27.1",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@types/node": "^24.7.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "antd": "^5.27.5",
    "chokidar": "^4.0.3",
    "playwright": "^1.56.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/babel__core": "^7.20.5",
    "@vitejs/plugin-react": "^5.0.4",
    "vite": "^7.1.10"
  }
}
