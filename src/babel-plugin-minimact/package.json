{
  "name": "babel-plugin-minimact",
  "version": "0.1.0",
  "description": "Babel plugin to transform JSX/TSX React components to C# Minimact components",
  "main": "index.cjs",
  "browser": "dist/minimact-babel-plugin.js",
  "module": "dist/minimact-babel-plugin.esm.js",
  "keywords": [
    "babel-plugin",
    "minimact",
    "react",
    "csharp",
    "jsx",
    "tsx"
  ],
  "author": "Minimact Contributors",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0"
  },
  "dependencies": {
    "@babel/types": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-react": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "rollup": "^3.29.0"
  }
}
