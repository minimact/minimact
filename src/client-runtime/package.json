{
  "name": "minimact-client",
  "version": "0.1.0",
  "description": "Client-side runtime for Minimact framework - Available in lightweight (SignalM) and full (SignalR) versions",
  "main": "dist/minimact.js",
  "module": "dist/minimact.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/minimact.esm.js",
      "require": "./dist/minimact.js",
      "default": "./dist/minimact.js"
    },
    "./minimact-r": {
      "types": "./dist/index-r.d.ts",
      "import": "./dist/minimact-r.esm.js",
      "require": "./dist/minimact-r.js",
      "default": "./dist/minimact-r.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest"
  },
  "keywords": [
    "minimact",
    "signalm",
    "signalr",
    "virtual-dom",
    "reactive",
    "dom-patching",
    "websocket",
    "real-time"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@microsoft/signalr": "^8.0.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.9.6",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "typescript": "^5.3.3"
  }
}
