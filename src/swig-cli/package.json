{
  "name": "@minimact/swig",
  "version": "1.0.0",
  "description": "CLI tool for Minimact development - Create, transpile, and run Minimact applications",
  "bin": {
    "swig": "./bin/swig.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/minimact/minimact.git",
    "directory": "src/swig-cli"
  },
  "bugs": {
    "url": "https://github.com/minimact/minimact/issues"
  },
  "homepage": "https://github.com/minimact/minimact#readme",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rimraf dist",
    "dev": "npm run build && node ./bin/swig.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "minimact",
    "swig",
    "cli",
    "transpiler",
    "tsx",
    "csharp",
    "aspnetcore",
    "server-side-rendering",
    "ssr",
    "dotnet"
  ],
  "author": "Minimact",
  "license": "MIT",
  "dependencies": {
    "@minimact/swig-shared": "^1.0.0",
    "commander": "^11.0.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.4",
    "typescript": "^5.3.0",
    "rimraf": "^5.0.0"
  }
}
